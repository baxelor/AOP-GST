            <xml-property name="queryText"><![CDATA[select
	product."name" as product_name,
	product.code as product_code,
	product.hsbn as hsbn_code,
	product.cost_price,
	product.sale_price,
	meta.file_name as product_image
from
	gst_product as product
left join meta_file as meta on
	meta.id = product.product_image
where
	product.id = ?]]></xml-property>
